<sui elementheight="50px">
			<layoutbox type="horizontal" stylesheet="LayoutboxNoborder">			
				<panel size="%elementheight%">
					<layoutbox type="vertical">
						<panel size="100px">
							<label text="Title"/>
						</panel>					
						<panel size="*">
							<textbox tag="title" width="100%" focus="true" />
						</panel>					
					</layoutbox>
				</panel>
				<panel size="%elementheight%">
					<layoutbox type="vertical">
						<panel size="100px">
							<label text="Template"/>
						</panel>					
						<panel size="*">
							<textbox tag="template" width="80%" disabled="true" />
							<button tag="choosetemplate" label="Select" width="20%" />
						</panel>											
					</layoutbox>
				</panel>
				<panel size="*">
				</panel>
			</layoutbox>
</sui>
